{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier",
        "react-hooks",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "pascalcase": "off",
        "no-undef": "warn",
        "no-unused-vars": "warn",
        "no-console": "warn",
        "format": [
            "error",
            "prettier"
        ],
        "prettier/prettier": [
            "error"
        ],
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "jsx-quotes": [
            "error",
            "prefer-single"
        ],
        "react/self-closing-comp": [
            "error",
            {
                "component": true,
                "html": true
            }
        ]
    }
}
